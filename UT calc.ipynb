    "#program to list out a dB table when a range is given\n",
    "\n",
    "print(\"Please enter the range for the dB table\")\n",
    "x = input('Start point')\n",
    "y = input('End value')\n",
    "\n",
    "a1 = int(x)\n",
    "y1 = int(y)\n",
    "\n",
    "\n",
    "\n",
    "for b in range(a1,y1):\n",
    "        c = 10**(b/20)\n",
    "        print(b,'dB','=', c)\n",
    "    \n",
    "\n",
    "\n",
    "\n"
